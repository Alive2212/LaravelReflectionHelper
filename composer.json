{
    "name": "alive2212/laravel-reflection-helper",
    "type": "library",
    "description": "This is Laravel Reflection Helper",
    "keywords": [
        "Alive2212",
        "LaravelReflectionHelper"
    ],
    "homepage": "https://github.com/Alive2212/LaravelReflectionHelper",
    "license": "MIT",
    "authors": [
        {
            "name": "Babak Nodoust",
            "email": "alive2212@yahoo.com",
            "homepage": "https://github.com/Alive2212",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": ">=8",
        "php" : "~5.6|~7.0|~8|~9"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Alive2212\\LaravelReflectionHelper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alive2212\\LaravelReflectionHelper\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
